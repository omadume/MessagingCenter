<div id="<%= dom_id passenger %>">
  <p>
    <strong>Name:</strong>
    <%= passenger.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= passenger.email %>
  </p>

  <p>
    <strong>Gender:</strong>
    <%= passenger.gender %>
  </p>

  <p>
    <strong>Date of birth:</strong>
    <%= passenger.date_of_birth %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= passenger.status %>
  </p>

</div>
